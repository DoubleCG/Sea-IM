				<div id='teamOption' class='sOption'>
				<span class="glyphicon glyphicon-fire" aria-hidden="true"></span>
				团队</div>
				<div id='strangeOption' class='sOption'>
				<span class="glyphicon glyphicon-pawn" aria-hidden="true"></span>
				陌生人</div>
			</div>
			<div id="messContent">
				<div id="starContent" class='sContent'>
				<ul></ul>
				</div>
				<div id="teamContent" class='sContent'>
				<ul></ul>
				</div>
