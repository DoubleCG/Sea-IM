	<link rel="stylesheet" href="/css/main.css">
	<audio id='tipvoice' src="voice/7499.wav"></audio>
</head>
<body>


<div class='container' id='main' >
<input style='display: none;' id='getuid' value='<%=uid%>'>
<input style='display: none;' id='getloginlist' value='<%=loginlist%>'>

	<div id="content" >
		<div id='getRender'></div>

		<domore-part uid="<%=uid%>"></domore-part>

		<div id="checkMess" >
			
			<user-info uid="<%=uid%>"  user_info="<%=user_info%>" ></user-info>
			<search-content uid='<%=uid%>'></search-content>

			<div id="messOption" v-on:click='hideDomore'>
				<div v-on:click='recentContentSeen' id='recentOption' class='sOption'>
				<span class="glyphicon glyphicon-star" aria-hidden="true"></span>
				recent</div>
				<div v-on:click='starContentSeen' id='starOption' class='sOption'>
				<span class="glyphicon glyphicon-star" aria-hidden="true"></span>
				朋友</div>
				<div v-on:click='teamContentSeen' id='teamOption' class='sOption'>
				<span class="glyphicon glyphicon-fire" aria-hidden="true"></span>
				团队</div>
				<div v-on:click='strangerContentSeen' id='strangerOption' class='sOption'>
				<span class="glyphicon glyphicon-pawn" aria-hidden="true"></span>
				陌生人</div>
			</div>

			<div id="messContent" v-on:click='hideDomore'>
				<div v-show='isMessContentSeen.recent' id="recentContent" class='sContent' >
					<mess-lis type='recent' info='<%=recentinfo%>'></mess-lis>
				</div>
				<div v-show='isMessContentSeen.star' id="starContent" class='sContent' >
					<mess-lis type='star' info='<%=starinfo%>'></mess-lis>
				</div>
				<div v-show='isMessContentSeen.team' id="teamContent" class='sContent'>
					<mess-lis type='team' info='<%=teaminfo%>'></mess-lis>
				</div>
				<div v-show='isMessContentSeen.stranger' id="strangerContent" class='sContent'>
					<mess-lis type='stranger' info='<%=strangerinfo%>'></mess-lis>
				</div>
			</div>
		</div>
	</div>


<div v-show='messageframeSeen' v-bind:class='{messageframe:true}'>
    <div v-bind:class='{messageframe_top:true}'>
      <div v-on:click='messageframe_close' v-bind:class='{messageframe_close:true}'>
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
      </div>
      <div>[{{messtype}}]{{nameOfmessageframe}}</div>
      <div v-bind:class='{messageframe_info:true}'>
        <span class="glyphicon glyphicon-menu-hamburger" aria-hidden="true"></span>
      </div>
    </div>
    <div id='messageframe_cont' v-bind:class='{messageframe_cont:true}'>
    	<div class="getMoreMess" v-on:click='getMoreMess'>Get More Message</div>
    </div>
    <div v-bind:class='{messageframe_say:true}'>
      <div v-bind:class='{messageframe_face:true}'></div>
      <input id='messageframe_input' v-on:keyup.13='sendMessage'></input>
      <button v-on:click='sendMessage' v-bind:class='{messageframe_subm:true}'>
        <span class="glyphicon glyphicon-share-alt" aria-hidden="true"></span>
      </button>
    </div>
  </div>
  
</div>








